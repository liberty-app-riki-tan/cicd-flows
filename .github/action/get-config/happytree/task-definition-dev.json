{
    "containerDefinitions": [
        {
            "name": "happytree",
            "image": "",
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:28081/health || exit 1"
                ],
                "interval": 5,
                "startPeriod": 20,
                "timeout": 2,
                "retries": 6
            },
            "environment": [
                {
                    "name": "JMX_PORT",
                    "value": "2046"
                },
                { "name": "APP_NAME", "value": "happytree"},
                {
                    "name": "DEBUG_ENABLE",
                    "value": "true"
                }
            ],
            "portMappings": [
                {
                    "hostPort": 2046,
                    "protocol": "tcp",
                    "containerPort": 2046
                },
                {
                    "hostPort": 28081,
                    "protocol": "tcp",
                    "containerPort": 28081
                },
                {
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "containerPort": 8080
                }
            ],
            "dockerLabels": {
                "ECS_EXPORTER_BIN_PROD_PORT": "2046",
                "ECS_EXPORTER_BIN_APP_PORT": "28081"
            },
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/usa-dev-ecs",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "/happytree",
                    "awslogs-datetime-format": "%Y\\.%m\\.%d"
                }
            }
        }
    ],
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "2048",
    "memory": "4096",
    "family": "happytree",
    "executionRoleArn": "arn:aws:iam::395583234375:role/ecsTaskExecutionRole",
}
